{% extends 'Mypage/mypage_frame.twig' %}

{% set mypageno = 'review' %}

{% set body_class = 'mypage' %}

{% block title %}{{ 'front.mypage.review.review_complete'|trans }}{% endblock %}

{% block content %}
<div class="ec-registerCompleteRole">
    <div class="ec-off2Grid ec-text-ac">
        <div class="ec-off2Grid__cell">
            <div class="ec-reportHeading">
                <h2>{{ 'front.mypage.review.completed_review'|trans }}</h2>
            </div>
            <p class="ec-reportDescription">{{ 'front.mypage.review.review_complete_message'|trans|nl2br }}</p>
            <div class="ec-off4Grid">
                <div class="ec-off4Grid__cell"><a class="ec-blockBtn--cancel"
                                                    href="{{ url('mypage') }}">{{ 'front.mypage.return_mypage'|trans }}</a>
                </div>
            </div>
        </div>
    </div>
</div>
{% endblock %}
