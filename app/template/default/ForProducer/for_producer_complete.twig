{% extends 'default_frame.twig' %}

{% block main %}
    <div class="ec-role">
        <div class="ec-pageHeader">
            <h1>{{'front.for_producer.title'|trans}}Ôºè{{ 'front.for_producer.request_completed_title'|trans }}</h1>
        </div>
    </div>
    <div class="ec-forProducerCompleteRole">
        <div class="ec-off2Grid ec-text-ac">
            <div class="ec-off2Grid__cell">
                <div class="ec-reportHeading">
                    <h2>{{ 'front.for_producer.request_completed_message_title'|trans }}</h2>
                </div>
                <p class="ec-reportDescription">{{ 'front.for_producer.request_completed_message_body'|trans|nl2br }}</p>
                <div class="ec-forProducerCompleteRole__support">
                    <p>{{ 'front.for_producer.contact_link'|trans({ '%url%': url('contact') })|raw  }}</p>
                    <p>{{'front.contact.phone_number'|trans}}</p>
                </div>
            </div>
        </div>
        <div class="ec-off4Grid">
            <div class="ec-off4Grid__cell"><a class="ec-blockBtn--cancel"
                                                href="{{ url('homepage') }}">{{ 'common.go_to_top'|trans }}</a>
            </div>
        </div>
    </div>
{% endblock %}


